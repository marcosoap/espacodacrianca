#family_activities {
  #banner {
    background: url(<%= image_path "activities/familias_banner.jpg" %>);
    height: 300px;
    background-size: cover;
    background-position: 0% 10%;
  }

  h2 {
    margin: 0px;
    font-size: 28px;
    color: #333;
    line-height: inherit;
  }

  ul, li {
    display: block;
    padding: 0px;
    margin: 0px;
  }
}