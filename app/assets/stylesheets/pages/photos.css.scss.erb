#photos {
  #banner {
    background-image: url(<%= image_path "pages/photos/banner.jpg" %>);
    height: 300px;
    background-size: cover;
    background-position: 50% 50%;
  }

  h2 {
    padding-top: 0px;
  }

  div.text {
    padding-top: 160px;
    text-align: center;
    font-size: 18px;
  }

  img {
    width: 140px;
  }
}